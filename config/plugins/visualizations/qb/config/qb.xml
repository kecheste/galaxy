<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE visualization SYSTEM "../../visualization.dtd">
<visualization name="QB" embeddable="false">
    <description>rejuve query builder</description>
    <data_sources>
        <data_source>
            <model_class>HistoryDatasetAssociation</model_class>
            <test type="isinstance" test_attr="datatype" result_type="datatype">binary.H5</test>
        </data_source>
    </data_sources>
    <params>
        <param type="dataset" var_name_in_template="hda" required="true">dataset_id</param>
    </params>
    <entry_point entry_point_type="chart" src="dist/index.js" css="dist/index.css"/>
    <settings>
        <input>
            <name>explorer</name>
            <value>false</value>
            <type>boolean</type>
        </input>
    </settings>
</visualization>